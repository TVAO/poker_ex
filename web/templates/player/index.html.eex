<div class="row">
  <div class="col s4 offset-s4">
    <h1 class="header">Players</h1>
  </div>
  <div class="col s4">
    
  </div>
  <ul class="collection">
    <%= for player <- @players do %>
      <li class="collection-item">
        <h4>Username: <%= player.name %></h4>
        <h4>Chips: <%= player.chips %></h4>
      </li>
    <% end %>
  </ul>
</div>